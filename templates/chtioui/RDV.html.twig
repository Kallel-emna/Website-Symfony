{% extends 'base(back).html.twig' %}
{% block body %}

<div class="card">
	<div class="card-body">
		<h5 class="card-title">Ajouter Rendez-vous</h5>

		<!-- General Form Elements -->
		{{ form_start(form,{attr:{novalidate:'novalidate'}}) }}
		<label for="inputText" class="col-sm-4 col-form-label">Nom</label>
		<div class="col-sm-10">
			{{ form_widget(form.time, {'attr': {'class': 'form-control'}}) }}
		</div>
		{{ form_errors(form.time,{'attr':{'class':'text-danger'}}) }}


		<label for="inputText" class="col-sm-4 col-form-label">Date</label>
		<div class="col-sm-10">
			{{ form_widget(form.date_rendezvous, {'attr': {'class': 'form-control'}}) }}

		</div>
		{{ form_errors(form.date_rendezvous,{'attr':{'class':'text-danger'}}) }}


		{{ form_end(form) }}

	</div>
</div>{%endblock%}
