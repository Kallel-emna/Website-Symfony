{% extends 'base(back).html.twig' %}


{% block body %}





				<div class="card">
					<div class="card-body">
						<h5 class="card-title">Ajouter Ordonnance</h5>

						<!-- General Form Elements -->
						{{ form_start(form,{attr:{novalidate:'novalidate'}}) }}
						<label for="inputText" class="col-sm-4 col-form-label">Nom</label>
						<div class="col-sm-10">
							{{ form_widget(form.nompatient, {'attr': {'class': 'form-control'}}) }}
						</div>
						{{ form_errors(form.nompatient,{'attr':{'class':'text-danger'}}) }}


						<label for="inputText" class="col-sm-4 col-form-label">Date</label>
						<div class="col-sm-10">
							{{ form_widget(form.date, {'attr': {'class': 'form-control'}}) }}

						</div>
						{{ form_errors(form.date,{'attr':{'class':'text-danger'}}) }}

						<label for="inputText" class="col-sm-4 col-form-label">Médicament</label>
						<div class="col-sm-10">
							{{ form_widget(form.medicament, {'attr': {'class': 'form-control'}}) }}

						</div>
						{{ form_errors(form.medicament,{'attr':{'class':'text-danger'}}) }}

						{{ form_end(form) }}

					</div>
				</div>

			</div>

<!-- End #main --></body><a href="{{ path('app_ordonnance_index') }}" class="btn btn-info" >Retour à la liste</a>{% endblock %}
